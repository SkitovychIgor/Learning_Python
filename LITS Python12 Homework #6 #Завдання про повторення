def find_most_frequent(text):
    import re
    text = str(text.lower())    #приводимо до єдиного регістру який потрібен на виході
    result = re.findall(r'\w+', text)   #створити список з усіма "словами" окрім символів та пробілів
    shukai_dict = {}
    answer = []
    for y in result:    #пройтися по кожному слову в списку яке створила ре
        if shukai_dict.get(y) is not None:    #якщо значення по ключу-слову не відсутнє відповідно має якесь значення
            shukai_dict[y] = shukai_dict.get(y) + 1    #то додати до цього значення +1 і присвоїти цьому значенню
        else:
            shukai_dict[y] = 1    #інакше утворити ключ:значення в словнику із значенням = 1
    max_value = max(shukai_dict.values()) #змінна для фіксації максимального значення повторюваності         
    answer = [key for key, count in shukai_dict.items() if count == max_value]
    print(sorted(answer))    #вивісти фінальний список відсортувавши "по дефолту за алфавітом"
