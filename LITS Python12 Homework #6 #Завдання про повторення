def find_most_frequent(text):
    import re
    text = str(text.lower())    #приводимо до єдиного регістру який потрібен на виході
    result = re.findall(r'\w+', text)   #створити список з усіма "словами" окрім символів та пробілів
    shukaidict = {}
    answer = []
    for y in result:    #пройтися по кожному слову в списку яке створила ре
        if shukaidict.get(y) != None:    #якщо значення по ключу-слову не відсутнє відповідно має якесь значення
            shukaidict [y] = shukaidict.get(y) + 1    #то додати до цього значення +1 і присвоїти цьому значенню
        else:
            shukaidict [y] = 1    #інакше утворити ключ:значення в словнику із значенням = 1
    for key in shukaidict:    #пройти по всіх ключах утвореного словника тобто по словах
        if shukaidict.get(key) == max(shukaidict.values()):    #якщо значення по ключу є максимальним 
            answer.append(key)    #то додати до вихідного списку ключ 
    print(sorted(answer))    #вивісти фінальний список відсортувавши "по дефолту за алфавітом"
