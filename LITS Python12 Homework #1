def myflat(lst, outlst):
    # проходимося по елементах списку lst
    for x in lst:
        # перевіряємо чи є елемент сам по собі списком
        if isinstance(x, list):
            ''' якщо є, то перезапускаємо функцію myflat
                з цим елементом у якості аргумента'''
            myflat(x, outlst)
        else:    # якщо елемент не список тобто ми дійшли до кінця підсписку
            outlst.append(x)    # тоді до списку outlst ми додаємо цей елемент
    # по закінченю for повернути накопичене значення списку outlst
    return outlst
lst = [[1, [2, [3]], 4, [[[6, 7]]]], 5]
''' вивести результат обчислення функції myflat
    з арг1- список списків,арг2-порожній список'''
print(myflat(lst, []))
